<%- include("./partials/header") %> 

<div class="result">
  <% let result = results[0]; %>
        <div class="row">
                <div class="col s12 m12">
                  <div class="card blue-grey darken-1 z-depth-4">
                    <div class="card-content black-text">
                    <h1 class="card-title"><%= result.query %></h1>
                      <h1><span><%= result.temp %> &#8451;</span> </h1>
                      <h5 class="test">Feels like: <span><%= result.feelsLikeData%> &#8451;</span> </h5>
                      <h5 class="test">Humidity: <span><%= result.humidityData%>%</span></h5>
                      <p><%= result.description %></p>
                      <img src=<%= result.iconURL %> alt="">
                    </div>
                  </div>
                </div>
        </div>
    <a href="/"><button class="waves-effect btn-large" style="margin: 3rem;">Check Other Location</button></a>
</div>
    
</div>
</body>
</html>
